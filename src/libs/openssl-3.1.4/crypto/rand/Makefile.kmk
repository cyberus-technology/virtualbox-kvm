SUB_DEPTH = ../../../../..
include $(KBUILD_PATH)/subheader.kmk

LIBRARIES += crypto_rand
crypto_rand_TEMPLATE = SubLibCrypto
crypto_rand_DEFS = OPENSSL_NO_DEPRECATED
crypto_rand_SOURCES = \
	prov_seed.c \
	rand_deprecated.c \
	rand_egd.c \
	rand_err.c \
	rand_lib.c \
	rand_meth.c \
	rand_pool.c \
	randfile.c
$(evalcall VBOX_OPENSSL_X86,crypto_rand)

include $(FILE_KBUILD_SUB_FOOTER)
