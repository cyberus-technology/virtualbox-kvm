   $(PATH_TARGET)/VBoxSupCat.dir/VBoxSup.cat: \
   		$(PATH_TARGET)/VBoxSupCat.dir/VBoxSup.inf \
   		$(PATH_TARGET)/VBoxSupCat.dir/VBoxSup.sys
	$(call MSG_TOOL,Inf2Cat,VBoxSup-inf,$@,$<)
	$(call VBOX_MAKE_CAT_FN, $(@D),$@)
